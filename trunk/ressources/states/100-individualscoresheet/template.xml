<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE itext SYSTEM "ressources/templates/itext.dtd" >
<itext creationdate="{CURRENT_TIME}"
	producer="{producer}"
	pagesize="A4"
	orientation="landscape"
	author="{author}"
	title="Feuille de marque"
	>
	<bloc::scoresheet>
	<!-- entete -->
	<table columns="3" width="100.0%" widths="20.0;40.0;40.0">
		<row>
			<!-- Logo du club -->
			<cell verticalalign="Middle" rowspan="2">
				<paragraph leading="18.0" align="Default">
					<image url="{LOGO_CLUB_URI}" plainwidth="90.0" plainheight="100.0"></image>
				</paragraph>
			</cell>
			<!-- Information club -->
			<cell verticalalign="Default">
				<paragraph size="12.0">
					<chunk fontstyle="italic">{INTITULE_CLUB}</chunk>
				</paragraph>
				<paragraph>
					<chunk size="12.0" fontstyle="normal">{INTITULE_CONCOURS}</chunk>
					<newline />
					<chunk size="12.0" fontstyle="normal">{VILLE_CLUB}, {DATE_CONCOURS}</chunk>
				</paragraph>
			</cell>
			<!-- Etiquette concurrent -->
			<cell verticalalign="Default">
				<paragraph>
					<chunk size="10">{cid}</chunk>
					<newline/>
					<chunk size="10">{cclub}</chunk>
					<newline/>
					<chunk size="10">{clicence}</chunk>
				</paragraph>
				<paragraph>
					<chunk size="10">{emplacement}</chunk>
				</paragraph>
			</cell>
		</row>
	</table>
	<newline />
	<!-- tableau point -->
	<table columns="{NB_SERIE}" width="100.0%" widths="{PERCENT_SERIES}" cellpadding="2.0">
		<row>
			<bloc::series>
			<cell>
				
				<table width="100.0%" widths="{COLS_SIZE}" cellpadding="1.0">
					<row>
						<cell colspan="9">
							<paragraph>
								<chunk fontstyle="bold" size="10">{INTITULE_SERIE}</chunk>
							</paragraph>
						</cell>
					</row>
					<row>
						<cell rowspan="2" horizontalalign="Center" top="true" bottom="true" right="true" left="true"><phrase size="10">Volée</phrase></cell>
						<cell colspan="{NB_FLECHE_PAR_VOLEE}" horizontalalign="Center" top="true" bottom="true" right="true" left="true"><phrase size="10">Points par fléche</phrase></cell>
						<cell rowspan="2" horizontalalign="Center" top="true" bottom="true" right="true" left="true"><phrase size="10">Total volée</phrase></cell>
						<cell rowspan="2" horizontalalign="Center" top="true" bottom="true" right="true" left="true"><phrase size="10">Total cumulé</phrase></cell>
						<cell rowspan="2" horizontalalign="Center" top="true" bottom="true" right="true" left="true"><phrase size="10">M</phrase></cell>
						<cell rowspan="2" horizontalalign="Center" top="true" bottom="true" right="true" left="true"><phrase size="10">10</phrase></cell>
						<cell rowspan="2" horizontalalign="Center" top="true" bottom="true" right="true" left="true"><phrase size="10">9</phrase></cell>
					</row>
					<row>
						<bloc::fleches>
						<cell horizontalalign="Center" top="true" bottom="true" right="true" left="true"><phrase size="10">{NUM_FLECHE}</phrase></cell>
						</bloc::fleches>
					</row>
					<bloc::volees>
					<row>
						<cell horizontalalign="Center" top="true" bottom="true" right="true" left="true">
							<paragraph>
								<chunk size="10">{NUM_VOLEE}</chunk>
							</paragraph>
						</cell>
						<bloc::pointsparfleche>
						<cell top="true" bottom="true" right="true" left="true"></cell>
						</bloc::pointsparfleche>
						<cell top="true" bottom="true" right="true" left="true"></cell>
						<cell top="true" bottom="true" right="true" left="true"></cell>
						<cell top="true" bottom="true" right="true" left="true"></cell>
						<cell top="true" bottom="true" right="true" left="true"></cell>
						<cell top="true" bottom="true" right="true" left="true"></cell>
					</row>
					</bloc::volees>
					<row>
						<cell colspan="5" top="true" bottom="true" right="true" left="true"><phrase size="10">Total série:</phrase></cell>
						<cell top="true" bottom="true" right="true" left="true"></cell>
						<cell top="true" bottom="true" right="true" left="true"></cell>
						<cell top="true" bottom="true" right="true" left="true"></cell>
						<cell top="true" bottom="true" right="true" left="true"></cell> 
					</row>
				</table>
			</cell>
			<cell>
			</cell>
			</bloc::series>
		</row>
		<!--<row>
			<cell></cell>
		</row>-->
	</table>
	<newpage/>
	</bloc::scoresheet>
</itext>
